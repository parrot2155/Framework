<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="add1" class="com.test02.Address">
		<constructor-arg name="name" value="박태욱"/>
		<constructor-arg name="addr" value="서울시 강남구"/>
		<constructor-arg name="phone" value="010-1111-1111"/>	<!-- index로 지정해도 되고, name으로 지정해도 된다. -->
	</bean>
	<bean id="add2" class="com.test02.Address">
		<constructor-arg value="김태욱"/>
		<constructor-arg value="경기도 수원시"/>
		<constructor-arg value="010-2222-2222"/>
	</bean>
	
	
	

</beans>
